cat > .gitignore << 'EOF'
# Python
__pycache__/
*.pyc
*.pyo
*.pyd
*.sqlite3

# Virtual env
.venv/
venv/

# OS/editor
.DS_Store
*.swp
.idea/
.vscode/

# Flask/instance stuff
instance/
*.log

# Local env/secrets
.env
.env.*

# Node (if any)
node_modules/

# Artifacts
dist/
build/

# Project-specific
static/uploads/
EOF

.env
*.env